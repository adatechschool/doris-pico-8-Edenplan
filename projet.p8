-- map

function draw_map()
map(0,0,0,0,128,64)
// 20=limitedex et 10limitedey
end

function check_flag(flag,x,y)
local sprite=mget(x,y)
return fget(sprite,flag)
end

function update_camera()
//perso va jusqu'au bout
-- du cadre 
//et la camera ne sort pas
-- du cadre
local camx=mid(0,p.x-9,127-15)
local camy=mid(0,p.y-11,63-15)
camera(camx*8,camy*8)
end

function next_title(x,y)
local sprite=mget(x,y)
mset(x,y,sprite+1)
end

function pick_up_heart(x,y)
next_title(x,y)
p.heart+=1
cls(6)
sfx(1)
end

function pick_up_food(x,y)
next_title(x,y)
p.food+=1
cls(12)
sfx(1)
end


