// jeu

function _init()
create_player()

end
 
function _update()
player_movement()
update_camera()

end

function _draw()
cls()
spr(45,30,30,2,2)
draw_map()
draw_player()

end

-->8
// map

function draw_map()
map(0,0,0,0,128,64)
// 20=limitedex et 10limitedey
end

function check_flag(flag,x,y)
local sprite=mget(x,y)
return fget(sprite,flag)
end

function update_camera()
camx=p.x-9
camy=p.y-9
camera(camx*8,camy*8)

end
-->8
//player 1

function create_player()
p={
x=8,
y=23,
sprite=45,46,61,62,2,2
}
end

function player_movement()
newx=p.x
newy=p.y

if (btnp(⬅️)) newx-=1
if (btnp(➡️)) newx+=1
if (btnp(⬆️)) newy-=1
if (btnp(⬇️)) newy+=1

if not check_flag(0,newx,newy) then
p.x=newx
p.y=newy
end 
end

function draw_player()
spr(p.sprite,p.x*8,p.y*8,2,2)
end
