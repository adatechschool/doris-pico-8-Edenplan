-- jeu

function _init()
start_time = time()
  duration = 60
//animation etoile:
s={}
for i=1,1000 do 
ns={x=rnd(1024),y=rnd(98)+376,
col=7,spd=rnd(3)+1}
add(s,ns)
end
create_player()
create_enemies()
end


function _update()

--player_animate()
player_movement()

--create_enemy()
update_camera()
for st in all (s) do
st.x-=st.spd --vitesse
if(st.x<0) then st.x=1024
end

end
