--time() - start_time
--local remaining_time =
 --duration - elapsed_time
 --if remaining_time <= 0 then
-- p.x = 8
-- p.y = 10
      --  return
  
 -- end
end

function _draw()

rectfill(0,0,127,26,12)


draw_map()
for st in all(s) do
pset(st.x,st.y, st.col)
end
draw_player()
draw_enemies()
enemies_movement()
draw_ui_cupcake()
draw_ui_heart()

local elapsed_time =
time() - start_time
 local remaining_time =
   duration - elapsed_time
   local seconds = remaining_time % 60
 print("temps restant: " .. seconds .. " ", 36, 4, 9)

end

